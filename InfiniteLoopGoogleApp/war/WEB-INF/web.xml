<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>edu.uwm.cs361.LoginServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>AddUser</servlet-name>
		<servlet-class>edu.uwm.cs361.AddUserServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Display</servlet-name>
		<servlet-class>edu.uwm.cs361.DisplayServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Clear</servlet-name>
		<servlet-class>edu.uwm.cs361.ClearServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>CreateInstructor</servlet-name>
		<servlet-class>edu.uwm.cs361.CreateInstuctorServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/addUser</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Display</servlet-name>
		<url-pattern>/display</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Clear</servlet-name>
		<url-pattern>/clear</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CreateInstructor</servlet-name>
		<url-pattern>/createInstructor</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
